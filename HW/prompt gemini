async function loadData() {
    const trainFile = document.getElementById('training-data').files[0];
    const testFile = document.getElementById('test-data').files[0];
    
    if (!trainFile || !testFile) {
        alert('Please upload both training and test CSV files.');
        return;
    }
    
    // ... rest of the function ...
}
